{
    "pytest class": {
        "prefix": "ptc",
        "body": [
            "class Test${1:CaseName}:",
            "    def test_${2:smoke}(${3:self}) -> None:",
            "        ${4:pass}$0"
        ]
    },
    "pytest test function": {
        "prefix": "ptt",
        "body": [
            "def test_${1:smoke}(${2:self}) -> None:",
            "    ${3:pass}$0"
        ]
    },
    "New typed function": {
        "prefix": "deft",
        "body": [
            "def ${1:name}(${2:args}) -> ${3:None}:",
            "    ${4:pass}$0"
        ]
    },
    "Pandas DataFrame": {
        "prefix": "dt",
        "body": [
            "${1:varname} = pd.DataFrame({",
            "    \"${2:column}\": $0",
            "})"
        ]
    }
}
